{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/packages/design-system": true,
    // "**/packages/mazic": true,
    "**/packages/mazic-server/pb_data": true,
    // "**/packages/mazic-server": true,
    // "**/packages/mazic-pocketbase": true,
    // "**/packages/mazic-pocketbase-ui": true,
    // "**/packages/mazic-prisma": true,
  },
  "terminal.integrated.defaultProfile.osx": null,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.sortOrderLexicographicOptions": "unicode",
  "editor.inlineSuggest.enabled": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "cSpell.words": ["lucide", "orderedmap"],
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
}
